{
  "name": "allset-ui",
  "version": "1.0.0",
  "description": "allset-web-app's beginner UI code base",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode production --config ./config/webpack.config.js",
    "dev": "webpack serve --config ./config/webpack.config.js",
    "fix:eslint": "eslint -c ./config/.eslintrc --ignore-path ./config/.eslintignore ./ --fix",
    "fix:prettier": "prettier --config ./config/.prettierrc --ignore-path ./config/.prettierignore --write \"./**/*.{js,jsx,json,html,scss}\"",
    "lint": "npm run fix:prettier && npm run fix:eslint",
    "preinstall": "npm run configGithooks",
    "configGithooks": "chmod ug+x config/githooks/* && git config core.hooksPath config/githooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allset-web-app/UI.git"
  },
  "keywords": [
    "allset"
  ],
  "author": "Subbiah Chandramouli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/allset-web-app/UI/issues"
  },
  "homepage": "https://github.com/allset-web-app/UI#readme",
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-loader": "8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "6.7.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.29.4",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "path": "0.12.7",
    "prettier": "^2.6.1",
    "style-loader": "3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  }
}
